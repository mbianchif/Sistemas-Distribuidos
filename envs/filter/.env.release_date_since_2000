# Input
INPUT_EXCHANGE_NAMES=sanitize-movies
INPUT_EXCHANGE_TYPES=fanout
INPUT_QUEUE_NAMES=filter-release_date_since_2000
INPUT_QUEUE_KEYS=

# Output
OUTPUT_EXCHANGE_NAME=filter-release_date_since_2000
OUTPUT_EXCHANGE_TYPE=fanout
OUTPUT_QUEUE_NAMES=join-right-id_id,filter-production_countries_argentina_spain,filter_production_countries_argentina
OUTPUT_QUEUE_KEYS=,,

# Worker
SELECT=id,title,genres,production_countries,release_date

# Filter
HANDLER=range
KEY=release_date
VALUE=2000,
