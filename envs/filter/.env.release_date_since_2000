# Input
INPUT_EXCHANGE_NAMES=sanitize-movies
INPUT_QUEUE_NAMES=filter-release_date_since_2000

# Output
OUTPUT_EXCHANGE_NAME=filter-release_date_since_2000
OUTPUT_QUEUE_NAMES=join-filter-id_id,filter-production_countries_argentina_spain,filter-production_countries_argentina
OUTPUT_DELIVERY_TYPES=shard:id,robin,robin

# Worker
SELECT=id,title,genres,production_countries,release_date

# Filter
HANDLER=range
KEY=release_date
VALUE=2000,
